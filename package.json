{
  "name": "bookstore-rdbms",
  "version": "0.0.1",
  "description": "A Bookstore project, RDBMS class, HCMUT",
  "main": "index.ts",
  "scripts": {
    "setup-dev": "npm i && cp .env.example .env",
    "start:dev-container": "docker compose -f docker-compose.dev.yaml up --build -d",
    "stop:dev-container": "docker compose -f docker-compose.dev.yaml down",
    "dev": "npx nodemon ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npx eslint --ext .ts --fix src",
    "lint:check": "npx eslint --ext .ts src",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "clean-dev": "rm -rf node_modules .env"
  },
  "author": "Thuy Nguyen",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "hbs": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
